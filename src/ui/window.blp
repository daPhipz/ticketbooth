using Gtk 4.0;
using Adw 1;

template $TicketboothWindow: Adw.ApplicationWindow {
  default-width: 1024;
  default-height: 600;
  width-request: 400;
  height-request: 600;

  map => $_on_map();

  Adw.ViewStack _win_stack {
    vexpand: true;
    hexpand: true;
  }
}
